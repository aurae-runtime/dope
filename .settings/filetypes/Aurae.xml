<filetype binary="false" default_extension="aurae" description="Aurae files" name="Aurae">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="//" />
      <option name="COMMENT_START" value="/*" />
      <option name="COMMENT_END" value="*/" />
      <option name="HEX_PREFIX" value="" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_BRACES" value="true" />
      <option name="HAS_BRACKETS" value="true" />
      <option name="HAS_PARENS" value="true" />
      <option name="HAS_STRING_ESCAPES" value="true" />
    </options>
    <keywords keywords="do;else;for;if;print;status;while" ignore_case="false" />
    <keywords2 keywords="catch;const;false;fn;import;let;return;throw;true;try" />
  </highlighting>
  <extensionMap>
    <mapping pattern="aurae.*" />
    <mapping ext="aurae" />
  </extensionMap>
</filetype>